<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dashboard"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/app_bg"
    android:fillViewport="true"
    android:scrollbars="none"
    tools:context=".MainActivity">

    <RelativeLayout
        android:id="@+id/school"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin">

        <View
            android:id="@+id/centerVerticalView"
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <View
            android:id="@+id/viewNotificationBg"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@drawable/view_bg" />

        <View
            android:id="@+id/viewRewardBg"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/viewNotificationBg"
            android:layout_marginTop="10dp"
            android:background="@drawable/view_bg" />

        <View
            android:id="@+id/viewGalleryBg"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/viewRewardBg"
            android:layout_marginTop="10dp"
            android:background="@drawable/view_bg" />


        <View
            android:id="@+id/viewEventBg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignBottom="@+id/viewGalleryBg"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/viewRewardBg"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            android:layout_toEndOf="@+id/viewRewardBg"
            android:layout_toRightOf="@+id/viewRewardBg"
            android:background="@drawable/view_bg" />

        <View
            android:id="@+id/viewAttendanceBg"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_alignEnd="@+id/viewFeesColor"
            android:layout_alignLeft="@+id/viewFeesColor"
            android:layout_alignRight="@+id/viewFeesColor"
            android:layout_alignStart="@+id/viewFeesColor"
            android:layout_alignTop="@+id/viewFeesColor"
            android:background="@drawable/view_bg" />

        <View
            android:id="@+id/viewAlertBg"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/viewGalleryBg"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_toLeftOf="@+id/viewAttendanceBg"
            android:layout_toStartOf="@+id/viewAttendanceBg"
            android:background="@drawable/view_bg" />


        <View
            android:id="@+id/viewParentZoneBg"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/viewAlertBg"
            android:layout_marginTop="10dp"
            android:background="@drawable/view_bg" />


        <View
            android:id="@+id/viewNotificationColor"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/dash_title_blue" />

        <TextView
            android:id="@+id/textViewNotificationTitle"
            style="@android:style/TextAppearance.Medium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/viewNotificationColor"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:paddingTop="3dp"
            android:text="Notifications"
            android:textColor="#ffffff" />

        <View
            android:id="@+id/viewEventColor"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_alignLeft="@+id/viewEventBg"
            android:layout_alignStart="@+id/viewEventBg"
            android:layout_alignTop="@+id/viewEventBg"
            android:background="@drawable/dash_title_blue" />


        <TextView
            android:id="@+id/textViewEventTitle"
            style="@android:style/TextAppearance.Medium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/viewEventColor"
            android:layout_alignLeft="@+id/viewEventBg"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignStart="@+id/viewEventBg"
            android:layout_alignTop="@+id/viewEventBg"
            android:gravity="center"
            android:paddingTop="3dp"
            android:text="Events"
            android:textColor="#ffffff" />

        <View
            android:id="@+id/viewRewardColor"
            android:layout_width="match_parent"
            android:layout_height="19dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/viewRewardBg"
            android:layout_toLeftOf="@+id/viewEventBg"
            android:layout_toStartOf="@+id/viewEventBg"
            android:background="@drawable/dash_title_blue" />


        <TextView
            android:id="@+id/textViewRewardTitle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignBottom="@+id/viewRewardColor"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/viewEventBg"
            android:layout_toLeftOf="@+id/viewEventBg"
            android:layout_toStartOf="@+id/viewEventBg"
            android:gravity="center"
            android:paddingTop="3dp"
            android:text="Rewards"
            android:textColor="#ffffff"
            android:textSize="10sp" />

        <ImageView
            android:id="@+id/rewardsImageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/viewGalleryBg"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/viewRewardColor"
            android:layout_toLeftOf="@+id/viewEventBg"
            android:layout_toStartOf="@+id/viewEventBg"
            android:onClick="openRewards"
            android:src="@drawable/trophy" />


        <View
            android:id="@+id/viewGalleryColor"
            android:layout_width="match_parent"
            android:layout_height="19dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/viewGalleryBg"
            android:layout_toLeftOf="@+id/viewEventBg"
            android:layout_toStartOf="@+id/viewEventBg"
            android:background="@drawable/dash_title_blue" />


        <TextView
            android:id="@+id/textViewGalleryTitle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignBottom="@+id/viewGalleryColor"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/viewGalleryBg"
            android:layout_toLeftOf="@+id/viewEventBg"
            android:layout_toStartOf="@+id/viewEventBg"
            android:gravity="center"
            android:text="Gallery"
            android:textColor="#ffffff"
            android:textSize="10sp" />


        <ImageView
            android:id="@+id/galleryClickImageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/viewAlertBg"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/viewGalleryColor"
            android:layout_toLeftOf="@+id/viewEventBg"
            android:layout_toStartOf="@+id/viewEventBg"
