        <ImageView
            android:id="@+id/imageButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/viewParentZoneBg"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/viewParentZonesColor"
            android:layout_toEndOf="@+id/alertTextThird"
            android:layout_toRightOf="@+id/alertTextThird"
            android:src="@drawable/dash_guage" />


        <com.github.lzyzsd.circleprogress.ArcProgress
            android:id="@+id/attendance_progress"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@android:color/transparent"
            custom:arc_bottom_text_size="20sp"
            custom:arc_finished_color="#2677AC"
            custom:arc_max="100"
            custom:arc_progress="0"
            custom:arc_stroke_width="10dp"
            custom:arc_suffix_text="%"
            custom:arc_suffix_text_padding="-3dp"
            custom:arc_text_color="#2677AC"
            custom:arc_text_size="22sp"
            custom:arc_unfinished_color="#6effffff"
            android:layout_below="@+id/viewParentZonesColor"
